{
  "compilerOptions": {
    "target": "es2021",
    "module": "nodenext",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@vitalets/global-cache": ["./src/index.ts"],
      "@vitalets/global-cache/server": ["./src/server/index.ts"]
    }
  },
  "include": ["**/*.ts"],
  "exclude": ["dist"]
}
